{
  "name": "webpack-react-redux-starter",
  "version": "0.1.0",
  "description": "Modern starter kit for React projects.",
  "author": "Aliaksandr Stsiarzhanau <sterzhanoff@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve:src": "cross-env NODE_ENV=development babel-node ./tools/srcServer.js",
    "serve:dist": "cross-env NODE_ENV=production babel-node ./tools/distServer.js",
    "clean:dist": "rimraf ./dist",
    "lint": "eslint ./src ./tools ./webpackConfig \"./*.js\"",
    "test": "mocha --opts ./mocha.opts \"./{,!(node_modules)/**/}*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "predev": "babel-node ./tools/startMessage.js",
    "dev": "run-p serve:src test:watch",
    "prebuild": "run-s clean:dist test",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "start": "npm run serve:dist"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-saga": "0.14.3"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-3": "6.22.0",
    "babel-register": "6.22.0",
    "browser-sync": "^2.18.6",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.4",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "eslint-watch": "^2.1.14",
    "html-webpack-plugin": "^2.26.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "npm-run-all": "4.0.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "next",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "keywords": [
    "es6",
    "react",
    "starter",
    "kit",
    "boilerplate",
    "hot",
    "live",
    "reload",
    "edit",
    "editing",
    "hmr",
    "webpack",
    "webpack 2",
    "browsersync",
    "browser-sync",
    "lint",
    "eslint",
    "unit",
    "test",
    "mocha",
    "enzyme",
    "jsdom"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stsiarzhanau/webpack-react-redux-starter.git"
  }
}
